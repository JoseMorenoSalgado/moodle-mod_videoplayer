<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/videoplayer/db" VERSION="2025071500" COMMENT="XMLDB file for Video Elearning Cloud module" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="videoplayer" COMMENT="Table for Video Elearning Cloud activity">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT=""/>
